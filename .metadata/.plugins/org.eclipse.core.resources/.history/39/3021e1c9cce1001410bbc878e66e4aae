public class Main {

	public static void main(String[] args) {
		ProducerConsumerWithThreadPool pc = new ProducerConsumerWithThreadPool();
		ThreadPool threadPool = pc.init();
		for (int i = 0; i < 100; i++) {
			pool.addTask(new Producer());
			pool.addTask(new Producer());
			pool.addTask(new Producer());

			pool.addTask(new Consumer());
			pool.addTask(new Consumer());
			pool.addTask(new Consumer());
		}
	}

}
